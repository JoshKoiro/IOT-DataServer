        link(rel="stylesheet" href="/css/graph.css")
    body(style="background-color:#ffffff;")
        include header.pug
        include graph.pug 
        include jumbo.pug
        div(class="row")
            div(class="col-5 offset-sm-1")
                include dataCard.firstEntry.pug 
            div(class="col-5")
